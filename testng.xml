<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SpiceJet">
	<test thread-count="5" name="Valid positive Test cases ">
		<parameter name="browser" value="Chrome" />
		<!--<parameter name="browser" value="Edge" />-->
		<classes>
			<class name="com.spicejet.utils.SeWrappers" />
			<class name="com.spicejet.utils.SeWrappers" />
			<class name="com.spicejet.test.SignUpTest_Positive" />
		</classes>
	</test>
	<test name="InValid Negative Test cases">
		<parameter name="browser" value="Chrome" />
		<!--<parameter name="browser" value="Edge" />-->
		<classes>
			<class name="com.spicejet.test.SignUpTest_Negative" />
		</classes>
	</test>
</suite> 